        <img class="header-static-bg-img" src="{{ asset('bundles/vcg/images/slider/flag.jpg') }}" />
    </div>
{% endblock %}
{% block breadcrumbs %}
    <a href="{{ path('page',{'slug':'culture-benefits'}) }}">UPS Culture &amp; Benefits</a>
    <a href="{{ path('culture_benefits_page', {'slug': 'history'}) }}">History</a>
{% endblock %}

