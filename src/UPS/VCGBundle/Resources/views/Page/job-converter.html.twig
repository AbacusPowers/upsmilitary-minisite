{% extends "VCGBundle:Layouts:page.html.twig" %}
{% block title %}Military Job Converter{% endblock %}
{% block javascripts %}
    {% javascripts 
        '@VCGBundle/Resources/public/js/vendor/jquery-csv/src/jquery.csv.js'
        '@VCGBundle/Resources/public/js/vendor/jquery.selectBoxIt/src/javascripts/jquery.selectBoxIt.js'
        '@VCGBundle/Resources/public/js/job-converter/form.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
{% block headerimage %}
    <div class="header-static-bg">
        <img class="header-static-bg-img" src="{{ asset('bundles/vcg/images/bg-5b.png') }}"    
    </div>
{% endblock %}
{% block breadcrumbs %}
