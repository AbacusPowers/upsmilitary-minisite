                                <div id="offsite-nav">
                                    <a id="forward-cancel" href="">Cancel</a>
                                    <a id="forward-to" href="" target="_blank">Continue</a>
                                </div>
                            </div>
                        {% endblock %}
                        <a href="#" id="close-offsite-modal"><i class="fa fa-times"></i></a>
                        {#<a href="{% block prevArticleLink %}{{ path(route, {'slug': video.Prev | raw })}}{% endblock %}" id="prev-article"><i class="fa fa-angle-left"></i></a>
